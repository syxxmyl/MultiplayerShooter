# Visual Studio 2015 user specific files
MPTesting/.vs/

MenuSystem/.vs/

Blaster/.vs/


# Compiled Object files
*.slo
*.lo
*.o
*.obj


# Precompiled Headers
*.gch
*.pch


# Compiled Dynamic libraries
*.so
*.dylib
*.dll


# Fortran module files
*.mod


# Compiled Static libraries
*.lai
*.la
*.a
*.lib


# Executables
*.exe
*.out
*.app
*.ipa


# These project files can be generated by the engine
*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb


# Precompiled Assets
MPTesting/SourceArt/**/*.png
MPTesting/SourceArt/**/*.tga

MenuSystem/SourceArt/**/*.png
MenuSystem/SourceArt/**/*.tga

Blaster/SourceArt/**/*.png
Blaster/SourceArt/**/*.tga


# Binary Files
MPTesting/Binaries/*
MPTesting/Plugins/*/Binaries/*

MenuSystem/Binaries/*
MenuSystem/Plugins/*/Binaries/*

Blaster/Binaries/*
Blaster/Plugins/*/Binaries/*


# Builds
MPTesting/Build/*

MenuSystem/Build/*

Blaster/Build/*


# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!MPTesting/Build/*/
MPTesting/Build/*/**
!MPTesting/Build/*/PakBlacklist*.txt

!MenuSystem/Build/*/
MenuSystem/Build/*/**
!MenuSystem/Build/*/PakBlacklist*.txt

!Blaster/Build/*/
Blaster/Build/*/**
!Blaster/Build/*/PakBlacklist*.txt


# Don't ignore icon files in Build
!MPTesting/Build/**/*.ico

!MenuSystem/Build/**/*.ico

!Blaster/Build/**/*.ico


# Built data for maps
*_BuiltData.uasset


# Configuration files generated by the Editor
MPTesting/Saved/*

MenuSystem/Saved/*

Blaster/Saved/*


# Compiled source files for the engine to use
MPTesting/Intermediate/*
MPTesting/Plugins/*/Intermediate/*

MenuSystem/Intermediate/*
MenuSystem/Plugins/*/Intermediate/*

Blaster/Intermediate/*
Blaster/Plugins/*/Intermediate/*


# Cache files for the editor to use
MPTesting/DerivedDataCache/*

MenuSystem/DerivedDataCache/*

Blaster/DerivedDataCache/*